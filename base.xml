<NEKTAR>
    <EXPANSIONS>
      <E COMPOSITE="C[32]" FIELDS="w" NUMMODES="6" TYPE="MODIFIED" /> 
    </EXPANSIONS>

    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="EQTYPE" VALUE="Poisson" />
            <I PROPERTY="Projection" VALUE="Continuous" />
        </SOLVERINFO>

        <VARIABLES>
            <V ID="0"> w </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
	    <B ID="0"> C[27] </B>  
	    <B ID="1"> C[28] </B>  
	    <B ID="2"> C[29] </B>  
	    <B ID="3"> C[30] </B>  
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
		    <D VALUE="0" VAR="w" />
            </REGION>
            <REGION REF="1">
		    <D VALUE="0.3" VAR="w" />
            </REGION>
            <REGION REF="2">
                <P VALUE="[3]" VAR="w" />
            </REGION>
            <REGION REF="3">
                <P VALUE="[2]" VAR="w" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="Forcing">
            <E VALUE="-2.0" VAR="w" />
        </FUNCTION>
    </CONDITIONS>
 
</NEKTAR>
